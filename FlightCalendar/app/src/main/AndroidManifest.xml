<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vumanh.flightcalendar" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Create"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".main_menu"
            android:label="@string/title_activity_main_menu" >
        </activity>
        <activity
            android:name=".navigation_drawer"
            android:label="@string/title_activity_navigation_drawer" >
        </activity>
        <activity
            android:name=".add_calendat"
            android:label="@string/title_activity_add_calendat" >
        </activity>
        <activity
            android:name=".add_calendar"
            android:label="@string/title_activity_add_calendar" >
        </activity>
        <activity
            android:name=".contact_page"
            android:label="@string/title_activity_contact_page" >
        </activity>
        <activity
            android:name=".add_contact"
            android:label="@string/title_activity_add_contact" >
        </activity>
    </application>

</manifest>
